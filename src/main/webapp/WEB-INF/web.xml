<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>HelloServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RegisterToDB</servlet-name>
    <servlet-class>RegisterToDB</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RegisterToDB</servlet-name>
    <url-pattern>/registerToDB</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LoginToOffice</servlet-name>
    <servlet-class>LoginToOffice</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginToOffice</servlet-name>
    <url-pattern>/loginToOffice</url-pattern>
  </servlet-mapping>


</web-app>